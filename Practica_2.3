// the setup routine runs once when you press reset:
void setup() {
  // initialize the digital pin as an output.

 pinMode(13, OUTPUT);
 pinMode(12, OUTPUT);
 pinMode(11, OUTPUT);
}

// the loop routine runs over and over again forever:
void loop() {
  
  digitalWrite(13, HIGH);
   digitalWrite(12, LOW);
    digitalWrite(11, HIGH);
    delay(500);
    digitalWrite(13, LOW);
    digitalWrite(12, HIGH);
    digitalWrite(11, LOW);
    delay(500
    );
}
